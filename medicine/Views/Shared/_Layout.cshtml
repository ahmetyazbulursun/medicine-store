<!DOCTYPE html>
<html dir="ltr" lang="tr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords"
          content="wrappixel, admin dashboard, html css dashboard, web dashboard, bootstrap 5 admin, bootstrap 5, css3 dashboard, bootstrap 5 dashboard, materialpro admin bootstrap 5 dashboard, frontend, responsive bootstrap 5 admin template, materialpro admin lite design, materialpro admin lite dashboard bootstrap 5 dashboard template">
    <meta name="description"
          content="Material Pro Lite is powerful and clean admin dashboard template, inpired from Bootstrap Framework">
    <meta name="robots" content="noindex,nofollow">
    <title>ZENX Medical</title>
    <link rel="canonical" href="https://www.wrappixel.com/templates/materialpro-lite/" />
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="64x64" href="~/material-pro-lite-master/assets/images/logo.ico">
    <!-- chartist CSS -->
    <link href="~/material-pro-lite-master/assets/plugins/chartist-js/dist/chartist.min.css" rel="stylesheet">
    <link href="~/material-pro-lite-master/assets/plugins/chartist-js/dist/chartist-init.css" rel="stylesheet">
    <link href="~/material-pro-lite-master/assets/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.css" rel="stylesheet">
    <!--This page css - Morris CSS -->
    <link href="~/material-pro-lite-master/assets/plugins/c3-master/c3.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/material-pro-lite-master/html/css/style.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css" referrerpolicy="no-referrer" />
</head>

<body>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
         data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <header class="topbar" data-navbarbg="skin6">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <div class="navbar-header" data-logobg="skin6">

                    <a class="navbar-brand ms-4" href="/AnaSayfa/Index">

                        <b class="logo-icon">
                            <img width="190" src="~/material-pro-lite-master/assets/images/white-logo.png" alt="homepage" class="img-fluid" />
                        </b>

                    </a>

                    <a class="nav-toggler waves-effect waves-light text-white d-block d-md-none"
                       href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
                </div>
                <!-- ============================================================== -->
                <!-- End Logo -->
                <!-- ============================================================== -->
                <div class="navbar-collapse collapse" id="navbarSupportedContent" data-navbarbg="skin5">
                    <ul class="navbar-nav d-lg-none d-md-block ">
                        <li class="nav-item">
                            <a class="nav-toggler nav-link waves-effect waves-light text-white "
                               href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
                        </li>
                    </ul>
                    <!-- ============================================================== -->
                    <!-- toggle and nav items -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav me-auto mt-md-0 ">
                        <!-- ============================================================== -->
                        <!-- Search -->
                        <!-- ============================================================== -->

                        <li class="nav-item search-box">
                            <a class="nav-link text-muted" href="javascript:void(0)"><i class="ti-search"></i></a>
                            <form class="app-search" style="display: none;">
                                <input type="text" name="ara" class="form-control" placeholder="Ara.."><a class="srh-btn"><i class="ti-close"></i></a>
                            </form>
                        </li>
                    </ul>

                    <!-- ============================================================== -->
                    <!-- Right side toggle and nav items -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav">
                        <!-- ============================================================== -->
                        <!-- User profile and search -->
                        <!-- ============================================================== -->
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-muted waves-effect waves-dark" href="/Admin/Index">
                                <img src="~/material-pro-lite-master/assets/images/user-logo.png" alt="user" class="profile-pic me-2"> @Session["KULLANICIADI"].ToString()
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown"></ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->

        @RenderBody()

        <aside class="left-sidebar" data-sidebarbg="skin6">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <!-- User Profile-->
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               href="/AnaSayfa/Index" aria-expanded="false">
                                <i class="mdi me-2 mdi-home"></i><span class="hide-menu">Ana Sayfa</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               href="/Urunler/Index" aria-expanded="false">
                                <i class="mdi me-2 mdi-medical-bag"></i><span class="hide-menu">Ürünler</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark"
                               href="/Siparis/Index" aria-expanded="false">
                                <i class="mdi me-2 mdi-basket"></i><span class="hide-menu">Siparişler</span>
                            </a>
                        </li>
                        <li id="cheques" class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark"
                               href="/Cekler/Index" aria-expanded="false">
                                <i class="mdi me-2 mdi-account-card-details"></i><span class="hide-menu">Çekler</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark"
                               href="/Raporlar/AylıkRapor" aria-expanded="false">
                                <i class="mdi me-2 mdi-calendar"></i><span class="hide-menu">Aylık Rapor</span>
                            </a>
                        </li>

                        
                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
            <div class="sidebar-footer">
                <div class="row">
                    <div class="col-4 link-wrap">
                        <!-- item-->
                        <a href="/Admin/Index" class="link" data-toggle="tooltip" title="" data-original-title="Settings">
                            <i class="ti-settings"></i>
                        </a>
                    </div>
                    <div class="col-4 link-wrap">
                        <!-- item-->
                        <a href="mailto:" class="link" data-toggle="tooltip" title="" data-original-title="Email">
                            <i class="mdi mdi-gmail"></i>
                        </a>
                    </div>
                    <div class="col-4 link-wrap">
                        <!-- item-->
                        <a href="/Login/Logout" class="link" data-toggle="tooltip" title="" data-original-title="Logout">
                            <i class="mdi mdi-power"></i>
                        </a>
                    </div>
                </div>
            </div>
        </aside>

        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->
    </div>

    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="~/material-pro-lite-master/assets/plugins/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/material-pro-lite-master/assets/plugins/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/material-pro-lite-master/html/js/app-style-switcher.js"></script>
    <!--Wave Effects -->
    <script src="~/material-pro-lite-master/html/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/material-pro-lite-master/html/js/sidebarmenu.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <!-- chartist chart -->
    <script src="~/material-pro-lite-master/assets/plugins/chartist-js/dist/chartist.min.js"></script>
    <script src="~/material-pro-lite-master/assets/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.min.js"></script>
    <!--c3 JavaScript -->
    <script src="~/material-pro-lite-master/assets/plugins/d3/d3.min.js"></script>
    <script src="~/material-pro-lite-master/assets/plugins/c3-master/c3.min.js"></script>
    <!--Custom JavaScript -->
    <script src="~/material-pro-lite-master/html/js/pages/dashboards/dashboard1.js"></script>
    <script src="~/material-pro-lite-master/html/js/custom.js"></script>



    @if (ViewBag.userAuthority != "Admin")
    {
        <script>
            var cheques = document.getElementById('cheques');

            cheques.setAttribute("hidden", "");
        </script>
    }
</body>

</html>